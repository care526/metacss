{
  "name": "@carelj/metacss",
  "version": "0.0.2",
  "description": "原子类的css库",
  "main": "index.js",
  "bin": {
    "metacss": "bin/care.js"
  },
  "scripts": {
    "dev": "npx node-sass ./metas/index.scss -w metas -o dev",
    "build": "npx node-sass ./metas/index.scss --output-style compressed -o dist",
